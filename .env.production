# Production Environment Configuration
ENVIRONMENT=production

# Server Configuration
PORT=3000
LOG_LEVEL=INFO
GIN_MODE=release

# Database Configuration
DB_HOST=postgres
DB_PORT=5432
DB_USER=asmo_prod_user
DB_PASSWORD=${DB_PASSWORD}
DB_NAME=asmo_production
DB_SSL_MODE=require

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# Monitoring
PROMETHEUS_METRICS=true

# CORS Configuration
ALLOWED_ORIGINS=https://need_to_change_domain.com,https://www.need_to_change_domain.com